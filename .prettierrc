{
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "printWidth": 1024
      }
    },
    {
      "files": "*.js",
      "options": {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 79,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false
      }
    }
  ]
}
